# AWS Configuration
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here
AWS_REGION=us-east-1

# S3 Vector Configuration (requires AWS S3 Vectors preview access)
S3_VECTOR_BUCKET_NAME=your-s3-vector-bucket-name
S3_VECTOR_INDEX_NAME=your-vector-index-name

# Vector Configuration
VECTOR_DIMENSION=384
EMBEDDING_MODEL=all-MiniLM-L6-v2

# File Validation Configuration
MAX_FILE_SIZE_MB=50
MAX_BATCH_SIZE_MB=200
ALLOWED_FILE_TYPES=text/*,application/pdf,image/*
BLOCKED_FILE_EXTENSIONS=.exe,.bat,.cmd,.scr,.com,.pif,.dll,.sys

# Service Configuration
HOST=0.0.0.0
PORT=8000
DEBUG=true

# Note: This implementation requires access to AWS S3 Vectors preview
# S3 Vectors is currently available in limited regions:
# - US East (N. Virginia)
# - US East (Ohio)  
# - US West (Oregon)
# - Asia Pacific (Sydney)
# - Europe (Frankfurt) 